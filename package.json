{
	"name": "vuecleanup",
	"displayName": "Vue Cleanup",
	"description": "Never get a PR sent back to you again",
	"publisher": "Taylor Plewe",
	"icon": "resources/vue-cleanup-logo-256.png",
	"version": "0.0.9",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Linters"
	],
	"keywords": [
		"FPS (First-Person Shooter)"
	],
	"activationEvents": [
		"onView:vuecleanup.unused-labels"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "vue-cleanup",
					"title": "Vue Cleanup",
					"icon": "resources/broom-solid.svg"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "vuecleanup.unused-labels",
				"contents": "Open a Vue (.vue) file to use this extension."
			}
		],
		"views": {
			"vue-cleanup": [
				{
					"id": "vuecleanup.unused-labels",
					"name": "Unused labels",
					"visibility": "visible"
				},
				{
					"id": "vuecleanup.misc",
					"name": "Misc",
					"visibility": "visible"
				},
				{
					"id": "vuecleanup.comments",
					"name": "Comments",
					"visibility": "collapsed"
				},
				{
					"id": "vuecleanup.todos",
					"name": "Todos",
					"visibility": "collapsed"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2",
		"@vscode/test-electron": "^2.1.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/taylorplewe/vuecleanup.git"
	}
}
